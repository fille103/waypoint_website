<%= link_to("<< Back to List", {:action => 'index', :waypoint_feature_id => @waypoint_feature.id, :admin_id => @waypoint_feature.admin_id}, :class => 'back-link') %>

<div class="feature_description show">
    <h2>Show Feature Description</h2>
    <table summary="Feature description detail view">
        <tr>
            <th>ID</th>
            <td><%= @feature_description.id %></td>
        </tr>
        <tr>
            <th>Description</th>
            <td><%= @feature_description.description %></td>
        </tr>
        <tr>
            <th>Waypoint Feature ID</th>
            <td><%= @feature_description.waypoint_feature_id %></td>
        </tr>
        <tr>
            <th>Admin ID</th>
            <td><%= @feature_description.admin_id %></td>
        </tr>
        <tr>
            <th>Created</th>
            <td><%= @feature_description.created_at %></td>
        </tr>
        <tr>
            <th>Updated</th>
            <td><%= @feature_description.updated_at %></td>
        </tr>
    </table>
</div>
