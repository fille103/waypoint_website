<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="admins show">
    <h2>Show Admin</h2>
    <table summary="Admin detail view">
        <tr>
            <th>ID</th>
            <td><%= @admin.id %></td>
        </tr>
        <tr>
            <th>Username</th>
            <td><%= @admin.username %></td>
        </tr>
        <tr>
            <th>Encrypted Password</th>
            <td><%= @admin.password_digest %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= @admin.email %></td>
        </tr>
        <tr>
            <th>Created</th>
            <td><%= @admin.created_at %></td>
        </tr>
        <tr>
            <th>Updated</th>
            <td><%= @admin.updated_at %></td>
        </tr>
    </table>
</div>
